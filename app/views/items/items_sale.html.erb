<p id="notice"><%= notice %></p>

<h1>Listing Sale Items</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @items_sale.each do |item| %>
      <tr>
        <td><%= item.split(",")[0] %></td>
        <td><%= item.split(",")[1] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
